package com.cognizant.shapes;


public class PrimeNos {
	public static void main(String[] args) {
		int i, n = 1, count; 
		System.out.println(" ");	
		while(n <= 150)
		{
			count = 0;
			i = 2;
		    while(i <= n/2 )
		    {
		    	if(n % i == 0)
		    	{
		    		count++;
		    		break;
		    	}
		    	i++;
		    }
		    if(count == 0 && n != 1 )
		    {
		    	System.out.print(n + ",");
		    }
		    n++;
		}
	}
}
